# OpenAI API Key (required for AI chat functionality)
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# AWS Configuration
AWS_REGION=us-west-2
AWS_PROFILE=default

# Optional: Custom stage name (dev, staging, prod)
STAGE=dev

# Optional: Custom table names (will be appended with stage)
USERS_TABLE_NAME=Users
MEDICAL_RECORDS_TABLE_NAME=MedicalRecords
CONVERSATIONS_TABLE_NAME=Conversations
REMINDERS_TABLE_NAME=Reminders 